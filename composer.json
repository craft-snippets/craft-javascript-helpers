{
    "name": "craftsnippets/javascript-helpers",
    "description": "Twig tools for working with Javascript in templates",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "javascript helpers"
    ],
    "support": {
        "docs": "https://github.com/craft-snippets/craft-javascript-helpers/blob/master/README.md",
        "issues": "https://github.com/craft-snippets/craft-javascript-helpers/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Pogorzelski",
            "homepage": "http://craftsnippets.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "craftsnippets\\javascripthelpers\\": "src/"
        }
    },
    "extra": {
        "name": "Javascript helpers",
        "handle": "javascript-helpers",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/craft-snippets/craft-javascript-helpers/blob/master/README.md",
        "components": {
            "javascriptHelpersService": "craftsnippets\\javascripthelpers\\services\\JavascriptHelpersService"
        },
        "class": "craftsnippets\\javascripthelpers\\JavascriptHelpers"
    }
}
